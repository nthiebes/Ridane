{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "plugins": [],
  "settings": {},
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "array-bracket-spacing": 1,
    "arrow-parens": 1,
    "arrow-spacing": 1,
    "block-scoped-var": 1,
    "block-spacing": 1,
    "brace-style": 0,
    "camelcase": 1,
    "comma-dangle": 1,
    "comma-spacing": 1,
    "comma-style": 1,
    // "complexity": 1,
    "computed-property-spacing": 1,
    "consistent-return": 1,
    "consistent-this": 1,
    "curly": 1,
    "default-case": 1,
    "dot-notation": 1,
    "eol-last": 1,
    "eqeqeq": 2,
    "func-call-spacing": 1,
    "generator-star-spacing": 1,
    "guard-for-in": 1,
    "key-spacing": 1,
    "keyword-spacing": 1,
    "lines-around-comment": 1,
    "max-depth": 1,
    "max-len": [
      1,
      {
        "code": 160
      }
    ],
    "max-nested-callbacks": 1,
    "max-params": 1,
    "new-cap": 1,
    "new-parens": 1,
    // "padding-line-between-statements": [
    //   1,
    //   {
    //     "blankLine": "always",
    //     "prev": ["const", "let", "var"],
    //     "next": "*"
    //   },
    //   {
    //     "blankLine": "never",
    //     "prev": ["const", "let", "var"],
    //     "next": ["const", "let", "var"]
    //   }
    // ],
    "no-alert": 1,
    "no-array-constructor": 1,
    "no-bitwise": 1,
    "no-extra-parens": 0,
    "no-caller": 1,
    "no-case-declarations": 1,
    "no-catch-shadow": 1,
    "no-confusing-arrow": 0,
    "no-continue": 1,
    "no-debugger": 1,
    "no-div-regex": 1,
    "no-duplicate-imports": 1,
    "no-else-return": 1,
    "no-empty-function": 1,
    "no-eq-null": 1,
    "no-eval": 1,
    "no-extend-native": 1,
    "no-extra-bind": 1,
    "no-floating-decimal": 1,
    "no-global-assign": 1,
    "no-implicit-coercion": 1,
    "no-implicit-globals": 1,
    "no-implied-eval": 1,
    "no-invalid-this": 0,
    "no-label-var": 1,
    "no-labels": 1,
    "no-lone-blocks": 1,
    "no-lonely-if": 1,
    "no-loop-func": 1,
    "no-magic-numbers": 0,
    "no-mixed-operators": 0,
    "no-mixed-spaces-and-tabs": 1,
    "no-multi-spaces": 1,
    "no-multi-str": 1,
    "no-multiple-empty-lines": 1,
    "no-negated-condition": 1,
    "no-nested-ternary": 1,
    "no-new": 1,
    "no-new-func": 1,
    "no-new-object": 1,
    "no-new-wrappers": 1,
    "no-octal-escape": 1,
    "no-param-reassign": 1,
    "no-plusplus": 0,
    "no-proto": 1,
    "no-restricted-globals": 1,
    "no-return-assign": 1,
    "no-script-url": 1,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-shadow": 1,
    "no-shadow-restricted-names": 1,
    "no-tabs": 1,
    "no-template-curly-in-string": 2,
    "no-throw-literal": 1,
    "no-trailing-spaces": 0,
    "no-undefined": 2,
    "no-underscore-dangle": 1,
    "no-unmodified-loop-condition": 1,
    "no-unneeded-ternary": 1,
    "no-unsafe-negation": 2,
    "no-unused-expressions": [
      1,
      {
        "allowShortCircuit": true
      }
    ],
    "no-use-before-define": 1,
    "no-useless-call": 1,
    "no-useless-computed-key": 1,
    "no-useless-concat": 1,
    "no-useless-constructor": 1,
    "no-useless-escape": 1,
    "no-useless-rename": 1,
    "no-var": 1,
    "no-void": 1,
    "no-whitespace-before-property": 1,
    "no-with": 1,
    "object-property-newline": 0,
    "object-shorthand": 0,
    "one-var": 0,
    "one-var-declaration-per-line": 1,
    "operator-linebreak": 1,
    "prefer-const": 1,
    "prefer-rest-params": 1,
    "prefer-spread": 1,
    "quote-props": 0,
    "quotes": [1, "single"],
    "radix": 1,
    "rest-spread-spacing": 1,
    "semi": 1,
    "semi-spacing": 1,
    "space-before-blocks": 1,
    "space-in-parens": [1, "never"],
    "space-infix-ops": 1,
    "space-unary-ops": 1,
    "spaced-comment": 1,
    "symbol-description": 1,
    "template-curly-spacing": 1,
    "unicode-bom": 1,
    "vars-on-top": 1,
    "wrap-iife": 1,
    "wrap-regex": 1,
    "yield-star-spacing": 1,
    "yoda": 1
  },
  "globals": {
    "io": true,
    "Howl": true
  }
}
